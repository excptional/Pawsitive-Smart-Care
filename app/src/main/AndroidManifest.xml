<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/pawsitive_app_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pawsitive_app_icon"
        android:supportsRtl="true"
        android:theme="@style/Base.Theme.Pawsitive"
        tools:targetApi="31">
        <activity
            android:name=".main_files.activities.Authentication"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main_files.activities.MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>